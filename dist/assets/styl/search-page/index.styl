.dropdown-menu.inner .divider
    display: block
    padding: 0
    height: 1px
    pointer-events: none
.btn-group.bootstrap-select
    width: auto !important
    button
        background-color: #82ca87;
        box-shadow: 0 10px 10px rgba(10,10,10, .7)
        -webkit-border-radius: 5px
        -moz-border-radius: 5px
        border-radius: 5px
        overflow: hidden
        border: none
.input-group.date
    background-color: #82ca87;
    box-shadow: 0 10px 10px rgba(10,10,10, .7)
    -webkit-border-radius: 5px
    -moz-border-radius: 5px
    border-radius: 5px
    overflow: hidden
    width: 200px
    color: #000
    .form-control,input-group-addon
        border: none