.circle-o
    text-align: center
    width: 45px
    height: 45px
    line-height: 25px !important
    border-radius: 100% !important
    background-color: #000 !important
    border: 1px solid #80C785 !important
    color: #80C785
.section
    position: relative
    &-title
        position: absolute
        left: -15px
        padding-left: 0
        height: 100%
        h2
            writing-mode: vertical-lr
            transform: rotate(180deg)
            margin: 0 0 0 -5px
            font-size: 2rem
            height: calc(100% - 22px)
